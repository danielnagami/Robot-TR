@model RobotTR.Portal.MVC.Models.AnalyzerResultViewModel

@{
    ViewData["Title"] = "RobotTR | Resultado";
}

<h1>Resultado:</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Mensagem:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Score):
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Score)
        </dd>
    </dl>
</div>